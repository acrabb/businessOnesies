<?xml version="1.0" encoding="utf-8"?>
<edu.berkeley.cs160.onesies.metaapp.MAElements.MAButton
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="200dp"
    android:layout_height="100dp"
    android:padding="0dp"
    >
    <!-- THESE SHOULD BE VIEWS THAT CUSTOM ELEMENTS HAVE. -->
    <View
        android:id="@+id/highlightOverlay"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:background="@color/highlightColor"
	    android:visibility="invisible"
        />
    
    <!-- BUTTON ONLY THINGS -->
    <include layout="@layout/e_link_badge"
        	android:visibility="gone" />
    
</edu.berkeley.cs160.onesies.metaapp.MAElements.MAButton>